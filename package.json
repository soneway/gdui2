{
  "name": "@soneway/gdui2",
  "version": "1.2.0",
  "title": "gdui2",
  "keywords": [
    "vue",
    "gdui2",
    "components"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "src",
    "vetur"
  ],
  "scripts": {
    "up": "rm -rf node_modules && dnpm i",
    "dev": "vuepress dev docs",
    "build": "vuepress build docs",
    "pack": "npm run build && cd docs/.vuepress && mv dist ../../docs-dist",
    "dist:css": "gulp --gulpfile build/build-style.js",
    "dist:components": "webpack --config build/webpack.components.js",
    "dist:utils": "webpack --config build/webpack.utils.js",
    "dist:mixins": "webpack --config build/webpack.mixins.js",
    "dist:index": "webpack --config build/webpack.index.js",
    "dist:js": "npm run dist:components && npm run dist:utils && npm run dist:mixins && npm run dist:index",
    "dist": "rm -rf dist && npm run dist:css && npm run dist:js",
    "pub": "npm run dist && dnpm publish",
    "pub:beta": "npm run dist && dnpm publish --tag beta",
    "lint": "eslint src --ext js,vue --fix",
    "lint-staged": "lint-staged"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{js,vue}": [
      "eslint src --ext js,vue --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/soneway/gdui2"
  },
  "author": "soneway",
  "license": "MIT",
  "dependencies": {
    "async-validator": "^1.11.0",
